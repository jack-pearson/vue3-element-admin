  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.4",
    "element-plus": "^2.4.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.4.5",
    "vue-i18n": "^9.8.0",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
