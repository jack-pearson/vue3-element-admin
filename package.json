    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "husky": ">=8",
    "lint-staged": ">=15",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "qs": "^6.11.2",
    "sass": "^1.69.7",
    "tailwindcss": "^3.4.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "lint-staged": {
    "*.{js,ts,tsx,vue,css,scss}": "eslint --max-warnings 5 --cache --fix",
